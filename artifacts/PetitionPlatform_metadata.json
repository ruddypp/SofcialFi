		"devdoc": {
			"details": "Core logic untuk petition voting platform - Hanya DAO member yang bisa buat petition - Petition bisa dibuat dengan Campaign Token atau bayar ETH - Siapa saja bisa sign petition (1 wallet = 1 signature) - Setiap 5 petition berbayar, creator dapat 1 Campaign Token reward",
			"errors": {
				"OwnableInvalidOwner(address)": [
					{
						"details": "The owner is not a valid owner account. (eg. `address(0)`)"
					}
				],
				"OwnableUnauthorizedAccount(address)": [
					{
						"details": "The caller account is not authorized to perform an operation."
					}
				],
				"ReentrancyGuardReentrantCall()": [
					{
						"details": "Unauthorized reentrant call."
					}
				]
			},
			"kind": "dev",
			"methods": {
				"closePetition(uint256)": {
					"details": "Tutup petition sebelum deadline Hanya creator atau owner yang bisa close"
				},
				"createPetition(string,string,uint256)": {
					"details": "Buat petition baru",
					"params": {
						"_description": "Deskripsi petition",
						"_durationInDays": "Durasi petition dalam hari  User bisa pakai Campaign Token (gratis) atau bayar dengan ETH Setiap 5 petition berbayar, user dapat 1 Campaign Token reward",
						"_title": "Judul petition"
					}
				},
				"getPetition(uint256)": {
					"details": "Get detail petition"
				},
				"getTotalPetitions()": {
					"details": "Get total petition yang ada"
				},
				"getUserPaidPetitionCount(address)": {
					"details": "Get jumlah petition berbayar yang sudah dibuat user"
				},
				"hasUserSigned(uint256,address)": {
					"details": "Check apakah user sudah sign petition tertentu"
				},
				"owner()": {
					"details": "Returns the address of the current owner."
				},
				"renounceOwnership()": {
					"details": "Leaves the contract without owner. It will not be possible to call `onlyOwner` functions. Can only be called by the current owner. NOTE: Renouncing ownership will leave the contract without an owner, thereby disabling any functionality that is only available to the owner."
				},
				"setCampaignFee(uint256)": {
					"details": "Update campaign fee (owner only)"
				},
				"setRewardThreshold(uint256)": {
					"details": "Update reward threshold (owner only)"
				},
				"signPetition(uint256)": {
					"details": "Sign petition (tanda tangan) Gratis dan siapa saja bisa sign 1 wallet hanya bisa sign 1x per petition"
				},
				"transferOwnership(address)": {
					"details": "Transfers ownership of the contract to a new account (`newOwner`). Can only be called by the current owner."
				},
				"withdraw()": {
					"details": "Withdraw ETH dari contract ke owner"
				}
			},
